{
  "name": "geojson-converter-kit",
  "version": "0.0.7",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cocaine-coder/geojson-converter"
  },
  "files": [
    "dist",
    "lib"
  ],
  "keywords": [
    "geojson",
    "wkt",
    "kml",
    "shpfile",
    "shp",
    "dbf",
    "converter"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsc && vite build && vite build --mode cjs",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "@types/proj4": "^2.19.0",
    "typescript": "~5.9.2",
    "vite": "^7.1.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.4"
  },
  "dependencies": {
    "@types/geojson": "^7946.0.16",
    "@zip.js/zip.js": "^2.8.10",
    "fast-xml-parser": "^5.3.2",
    "iconv-lite": "^0.7.0",
    "proj4": "^2.20.0",
    "buffer": "^6.0.3",
    "stream-browserify": "^3.0.0"
  }
}
